<%= render_navigation context: 'my', :renderer => :breadcrumbs %>

<h1>
  <%= t("my.archive") %>
</h1>

<div class='my_afisha_wrapper'>
  <div class='my_afishas_preview'>
    <ul class='my_afisha_items'>
      <% current_user.afisha.archive.published.each do |afisha| %>
        <%= render :partial => 'afisha_list', :locals => { :afisha => afisha, :editable => true } %>
      <% end %>
    </ul>
  </div>
</div>
