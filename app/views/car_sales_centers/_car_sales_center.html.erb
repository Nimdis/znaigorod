<div class="suborganization_info car_sales_center">
  <table>
    <tr>
      <td class='border'>
        <%= car_sales_center.htmlise_title_on_show %>
      </td>
      <td>
        <%= car_sales_center.iconize_info %>
        <div class="details">
          <%= car_sales_center.htmlise_offers_and_features_on_show %>
        </div>
      </td>
    </tr>
  </table>
  <% if car_sales_center.has_photogallery? %>
    <div class="photogallery">
      <ul>
        <%= render car_sales_center.gallery_images, :rel => "car_sales_center_gallery_#{car_sales_center.id}" %>
      </ul>
    </div>
  <% end %>
</div>
